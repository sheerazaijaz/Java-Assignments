import java.util.Scanner;

public class code5 {
    public static void main(String[] args) {

        Scanner scan = new Scanner(System.in);

        System.out.println("Enter Voltage (Volt)");
        double volt = scan.nextDouble();

        System.out.println("Enter Current (Ampere)");
        double current = scan.nextDouble();

        double power = volt * current;

        System.out.println("Power = " + power + " Watt");
    }
}
